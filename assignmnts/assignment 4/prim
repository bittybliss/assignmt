var readlineSync = require("readline-sync");
let num = readlineSync.question("Enter an input:");
num = parseInt(num);
var  i, j=0;
for(i=2; i<num; i++)
{
  if(num%2==0)
  {
    j++;
    break;
  }
}
if(j==0)
{
  console.log(num + " is a Prime Number");
}
else
{
  console.log(num + " is not a Prime Number");
}